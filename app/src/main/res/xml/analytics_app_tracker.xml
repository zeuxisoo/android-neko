<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="TypographyDashes">
    <!-- How long a session exists before giving up -->
    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!--  The following value should be replaced with correct property id. -->
    <string name="ga_trackingId">UA-57199957-1</string>

    <!-- Percentage of events to include in reports -->
    <string name="ga_sampleFrequency">100.0</string>

    <!-- catch and report uncaught exceptions from the app -->
    <bool name="ga_reportUncaughtExceptions">true</bool>

    <!-- The screen names that will appear in reports -->
    <screenName name="im.after.app.AppStart">AppStart</screenName>
    <screenName name="im.after.app.ui.LoginActivity">LoginActivity</screenName>
    <screenName name="im.after.app.ui.MainActivity">MainActivity</screenName>
    <screenName name="im.after.app.ui.SettingsActivity">SettingsActivity</screenName>

    <screenName name="im.after.app.ui.module.article.ArticleFragment">ArticleFragment</screenName>
    <screenName name="im.after.app.ui.module.article.ArticleCreateActivity">ArticleCreateActivity</screenName>
    <screenName name="im.after.app.ui.module.article.ArticleEditActivity">ArticleEditActivity</screenName>
    <screenName name="im.after.app.ui.module.article.ArticleFullActivity">ArticleFullActivity</screenName>

    <screenName name="im.after.app.ui.module.memo.MemoFragment">MemoFragment</screenName>
    <screenName name="im.after.app.ui.module.memo.MemoCreateActivity">MemoCreateActivity</screenName>
    <screenName name="im.after.app.ui.module.memo.MemoEditActivity">MemoEditActivity</screenName>

    <screenName name="im.after.app.ui.module.talk.TalkFragment">TalkFragment</screenName>
    <screenName name="im.after.app.ui.module.talk.TalkCreateActivity">TalkCreateActivity</screenName>
    <screenName name="im.after.app.ui.module.talk.TalkEditActivity">TalkEditActivity</screenName>
</resources>